package interview_programs;

import java.util.Scanner;

public class pro1 {
	public static void main(String args[]) {
		int a=39;
		int b=22;
		System.out.println("Before swapping ");
		System.out.println(a);
		System.out.println(b);
		System.out.println("after swapping");
		int temp;
		temp=a;
		a=b;
		b=temp;
		System.out.println(a);
		System.out.println(b);
				
		
	}

}
