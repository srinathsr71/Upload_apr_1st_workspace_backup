{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": { "type": "integer" },
    "email": { "type": "string", "format": "email" },
    "first_name": { "type": "string" },
    "last_name": { "type": "string" },
    "avatar": { "type": "string", "format": "uri" }
  },
  "required": ["id", "email", "first_name", "last_name", "avatar"]
}
